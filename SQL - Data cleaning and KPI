SELECT 
    order_id,
    order_date,
    customer_id,
    product,
    category,
    quantity,
    price,
    (quantity * price) AS revenue,
    region
FROM sales
WHERE quantity > 0 AND price > 0;


#KPIs

SELECT 
    DATE_TRUNC('month', order_date) AS month,
    SUM(quantity * price) AS monthly_revenue
FROM sales
GROUP BY month
ORDER BY month;
